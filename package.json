    "planner",
    "trips"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.43",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postcss": "^8.5.6",
    "preline": "^3.2.3",
    "sequelize": "^6.35.2",
